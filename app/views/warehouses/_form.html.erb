<%= form_for(@warehouse) do |f| %>
  <% if @warehouse.errors.any? %>
    <div class="alert alert-error">
    	<button type="button" class="close" data-dismiss="alert">&times;</button>
      <h4><%=t('errors.title')%></h4>
      <ul>
      <% @warehouse.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.label :name, t(:name) %>
    <%= f.text_field :name %>
  </div>
  <div class="field">
    <%= f.label :address, t(:address) %>
    <%= f.text_field :address %>
  </div>
  <div class="field">
    <%= f.label :external_key, t(:external_key) %>
    <%= f.text_field :external_key %>
  </div>
  <div class="field form-inline">
  	<%= f.check_box :validity %>
    <%= f.label :validity, t(:validity) %>    
  </div>
  <div class="actions">
    <%= f.submit(t(:save), class: 'btn') %>
  </div>
<% end %>
